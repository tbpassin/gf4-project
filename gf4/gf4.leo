    _m.ydata = [y - x for x, y in zip(_my, _by)]

    lab = self.stack[MAIN].figurelabel or ''
    lab1 = self.stack[BUFFER].figurelabel or ''
    if lab and lab1:
        self.stack[MAIN].figurelabel = '%s - %s' % (lab1, lab)
