<t tx="tom.20211211170820.11">def dedup(self):
    """Remove data point if its value equals the previous value."""
    dedup = []
    last = None
    for i, y in enumerate(self.ydata):
        if y != last:
            last = y
            dedup.append((self.xdata[i], y))
    self.xdata, self.ydata = zip(*dedup)

